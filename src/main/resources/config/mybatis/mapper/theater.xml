<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="t">
	<select id="selectOne" resultType="theater" parameterType="String">
		SELECT * FROM THEATER_INFO
		WHERE area= #{area}
	</select>
</mapper>












